package com.axa.labs09;

public class Map {
	private int xSize, ySize;
	
	public Map(int xSize, int ySize) {
		this.xSize = xSize;
		this.ySize = ySize;
	}
	
	public int getXSize() {
		return xSize;
	}
	public int getySize() {
		return ySize;
	}
	public void setXSize(int xSize) {
		this.xSize = xSize;
	}
	public void setYSize(int ySize) {
		this.ySize = ySize;
	}
}
